
    <table summary="Prisoner form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
      <tr>
        <th>Password</th>
        <td><%= f.password_field(:password) %></td>
      </tr>
      <tr>
        <th>Password (confirmation)</th>
        <td><%= f.password_field(:password_confirmation) %></td>
      </tr>
      <tr>
        <th>IsAdmin</th>
        <td><%= f.check_box(:is_admin) %></td>
      </tr>
    </table>
    

